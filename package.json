{
  "name": "bashscript",
  "version": "1.0.0",
  "main": "index.js",
  "author": "Bazyli BrzoÃÅska <bazyli.brzoska@gmail.com>",
  "license": "MIT",
  "private": false,
  "scripts": {
    "start": "ts-node src/index",
    "ide": "ts-node $NODE_DEBUG_OPTION src/index",
    "test": "jest",
    "build:modules": "tsc --project . --outDir dist/es --module esnext",
    "build:commonjs": "tsc --project . --outDir dist/commonjs",
    "jest-debug": "node --inspect-brk node_modules/.bin/jest --runInBand",
    "pretty": "prettier --write \"./src/**/*.ts\"",
    "madge": "madge --circular src/index.ts",
    "preparcel:repl": "parcel build --out-file editor.worker.js node_modules/monaco-editor/esm/vs/editor/editor.worker && mv dist/editor.worker*.js dist/editor.worker.js && parcel build --out-file ts.worker.js node_modules/monaco-editor/esm/vs/language/typescript/ts.worker && mv dist/ts.worker*.js dist/ts.worker.js",
    "parcel:repl": "node --max-old-space-size=4096 node_modules/.bin/parcel serve repl/index.html",
    "repl:watch": "wp --watch",
    "repl:build": "del docs && yarn webpack",
    "webpack": "NODE_ENV=production wp",
    "postinstall": "patch-package"
  },
  "dependencies": {
    "acorn": "7.1.1",
    "prettier": "1.18.2",
    "ramda": "0.26.1",
    "ts-morph": "^4.3.2",
    "tslib": "1.10.0",
    "typescript": "^3.7.0-beta"
  },
  "devDependencies": {
    "@types/acorn": "4.0.5",
    "@types/jest": "24.0.20",
    "@types/node": "^12.11.7",
    "@types/ramda": "0.26.33",
    "crypto-browserify": "^3.12.0",
    "css-loader": "3.2.0",
    "del-cli": "3.0.0",
    "html-webpack-plugin": "^4.0.0-beta.8",
    "jest": "24.9.0",
    "madge": "3.4.4",
    "monaco-editor": "0.18.1",
    "monaco-editor-webpack-plugin": "1.7.0",
    "parcel-bundler": "1.12.4",
    "patch-package": "^6.2.0",
    "path-browserify": "^1.0.0",
    "postinstall-postinstall": "^2.0.0",
    "process": "^0.11.10",
    "raw-loader": "3.1.0",
    "semantic-release": "15.13.28",
    "stream-browserify": "^2.0.2",
    "style-loader": "1.0.0",
    "ts-jest": "24.1.0",
    "ts-loader": "6.2.1",
    "ts-node": "8.4.1",
    "vm-browserify": "^1.1.0",
    "webpack": "^5.0.0-beta.1",
    "webpack-nano": "^0.8.1",
    "webpack-plugin-serve": "^0.12.1"
  },
  "resolutions": {
    "chalk": "2.4.2"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.tsx?$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  },
  "prettier": {
    "semi": false,
    "singleQuote": true,
    "trailingComma": "all",
    "bracketSpacing": false,
    "arrowParens": "always",
    "parser": "typescript"
  }
}
