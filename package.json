{
  "name": "bashscript",
  "version": "1.0.0",
  "main": "index.js",
  "author": "Bazyli BrzoÃÅska <bazyli.brzoska@gmail.com>",
  "license": "MIT",
  "private": false,
  "scripts": {
    "start": "ts-node src/index",
    "ide": "ts-node $NODE_DEBUG_OPTION src/index",
    "test": "jest",
    "build:modules": "tsc --project . --outDir dist/es --module esnext",
    "build:commonjs": "tsc --project . --outDir dist/commonjs",
    "jest-debug": "node --inspect-brk node_modules/.bin/jest --runInBand",
    "pretty": "prettier --write \"./src/**/*.ts\"",
    "madge": "madge --circular src/index.ts",
    "preparcel:repl": "parcel build --out-file editor.worker.js node_modules/monaco-editor/esm/vs/editor/editor.worker && mv dist/editor.worker*.js dist/editor.worker.js && parcel build --out-file ts.worker.js node_modules/monaco-editor/esm/vs/language/typescript/ts.worker && mv dist/ts.worker*.js dist/ts.worker.js",
    "parcel:repl": "node --max-old-space-size=4096 node_modules/.bin/parcel serve repl/index.html",
    "repl:watch": "wp --watch",
    "repl:build": "del docs && yarn webpack",
    "webpack": "NODE_ENV=production wp",
    "postinstall": "patch-package"
  },
  "dependencies": {
    "acorn": "8.0.1",
    "mvdan-sh": "^0.5.0",
    "prettier": "2.0.5",
    "ramda": "0.27.1",
    "ts-morph": "^7.3.0",
    "tslib": "2.0.1",
    "typescript": "^4.0.0-beta"
  },
  "devDependencies": {
    "@types/acorn": "4.0.5",
    "@types/jest": "26.0.10",
    "@types/mvdan-sh": "^0.5.0",
    "@types/node": "^14.0.27",
    "@types/ramda": "0.27.14",
    "crypto-browserify": "^3.12.0",
    "css-loader": "4.2.1",
    "del-cli": "3.0.1",
    "file-loader": "^6.0.0",
    "html-webpack-plugin": "^4.3.0",
    "jest": "26.4.0",
    "jest-circus": "^26.4.0",
    "madge": "3.9.2",
    "monaco-editor": "0.20.0",
    "monaco-editor-webpack-plugin": "1.9.0",
    "parcel-bundler": "1.12.4",
    "patch-package": "^6.2.2",
    "path-browserify": "^1.0.1",
    "postinstall-postinstall": "^2.1.0",
    "process": "^0.11.10",
    "raw-loader": "4.0.1",
    "semantic-release": "17.2.3",
    "stream-browserify": "^3.0.0",
    "style-loader": "1.2.1",
    "ts-jest": "26.2.0",
    "ts-loader": "8.0.2",
    "ts-node": "8.10.2",
    "vm-browserify": "^1.1.2",
    "webpack": "^5.0.0-beta.26",
    "webpack-nano": "^1.0.0",
    "webpack-plugin-serve": "^1.0.1"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testEnvironment": "node",
    "testRunner": "jest-circus/runner",
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.tsx?$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  },
  "prettier": {
    "semi": false,
    "singleQuote": true,
    "trailingComma": "all",
    "bracketSpacing": false,
    "arrowParens": "always",
    "parser": "typescript"
  }
}
